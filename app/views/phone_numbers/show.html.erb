<%- model_class = PhoneNumber -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:p_type) %></dt>
      <dd><pre class="prettyprint"><%= @phone_number.p_type %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:p_number) %></dt>
      <dd><pre class="prettyprint"><%= @phone_number.p_number %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:contact_id) %></dt>
      <dd><pre class="prettyprint"><%= @phone_number.contact_id %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              phone_numbers_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_phone_number_path(@phone_number), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              phone_number_path(@phone_number),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>