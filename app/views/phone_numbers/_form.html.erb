<%= form_for @phone_number, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="form-group">
    <%= f.label :p_type, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :p_type, :class => 'text_field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :p_number, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :p_number, :class => 'text_field form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :contact_id, :class => 'control-label col-md-2' %>
    <div class="col-md-10">
      <%= f.text_field :contact_id, :class => 'text_field form-control' %>
    </div>
  </div>

  <div class="form-group">
    <div class='col-md-offset-2 col-md-10'>
      <%= f.submit nil, :class => 'btn btn-primary' %>
      <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                  phone_numbers_path, :class => 'btn btn-default' %>
    </div>
  </div>
<% end %>
